<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.60" fx:controller="com.neet.DiamondHunter.MainApp.LaunchController">
   <children>
      <Button fx:id="btnMapViewer" layoutX="175.0" layoutY="257.0" mnemonicParsing="false" onAction="#mapViewerLaunch" prefHeight="44.0" prefWidth="230.0">
      	<graphic>
        	<ImageView fitHeight="44.0" fitWidth="266.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="/CSSImages/btnMapViewer.gif" />
               </image>
            </ImageView>
        </graphic>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="btnPlay" layoutX="175.0" layoutY="185.0" mnemonicParsing="false" onAction="#gameDH" prefHeight="44.0" prefWidth="230.0">
      	<graphic>
        	<ImageView fitHeight="44.0" fitWidth="266.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="/CSSImages/btnPlay.gif" />
               </image>
            </ImageView>
        </graphic>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
	  <ImageView fitHeight="132.0" fitWidth="339.0" layoutX="142.0" layoutY="26.0" pickOnBounds="true" preserveRatio="true">
		   <image>
		      <Image url="/CSSImages/labelTitle.gif" />
		   </image>
	  </ImageView>
      <Button fx:id="btnExit" layoutX="175.0" layoutY="328.0" mnemonicParsing="false" onAction="#exit" prefHeight="60.0" prefWidth="252.0">
         <graphic>
            <ImageView fitHeight="34.0" fitWidth="101.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="/CSSImages/btnExit.gif" />
               </image>
            </ImageView>
         </graphic>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
